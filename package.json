{
  "name": "pharmex-satici-panel-pro",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:api": "nodemon ../pharmex-api/src/server.js",
    "dev:all": "concurrently -k \"npm:dev:api\" \"npm:dev\"",
    "build": "vite build",
    "preview": "vite preview --host",
    "clean": "rimraf node_modules .vite dist",
    "backup": "zip -r backup-$(date +%Y%m%d-%H%M).zip . -x \"node_modules/*\" \"dist/*\""
  },
  "dependencies": {
    "dompurify": "^3.2.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.0.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "vite": "^5.4.0"
  }
}
